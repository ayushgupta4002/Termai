{
    "devCommand": "uvicorn server:app --host 0.0.0.0 --port 3000",
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/server.py"
      }
    ],
    "functions": {
        "api/**": {
          "excludeFiles": "{*.cache,dist,public,app.src}/**"
        }
      }
    
  }
  